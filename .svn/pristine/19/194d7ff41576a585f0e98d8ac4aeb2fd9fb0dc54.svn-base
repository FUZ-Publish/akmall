.list_paging { position: relative; z-index: 10; height: 44px; border-top: 1px solid #e1e4e7; border-bottom: 1px solid #e5e5e5; background-color: #fff; font-size: 0; line-height: 0; text-align: center; margin-bottom: 8px;}
.list_paging button.btn_pre { left: 0; border-right: 1px solid #edf0f2;}
.list_paging button { overflow: hidden; line-height: 0; font-size: 0;color: transparent; vertical-align: top; display: block; position: absolute; top: 0; width: 43px; height: 44px; border: 0; background: none;}
.list_paging button.btn_pre:before { background-position: -44px -141px;}
.list_paging button.btn_pre.none:before { background-position: -35px -141px;}
.list_paging button:before { display: block; overflow: hidden; position: absolute; left: 17px; top: 14px; width: 9px; height: 16px; content: '';}
.list_paging button.btn_next:before { background-position: -65px -141px;}
.list_paging button.btn_next.none:before { background-position: -55px -141px;}
.list_paging button.btn_next { right: 0; border-left: 1px solid #edf0f2;}
.list_paging a.now { font-weight: bold; color: #e20265;}
.list_paging a { display: inline-block; width: 41px; height: 44px; font-size: 16px; line-height: 44px; color: #888;}

