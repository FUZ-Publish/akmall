.review_img_wrap {position:relative;margin-top:9px; }
.review_img_wrap .swiper-container-horizontal {height: 100%;}
.review_img_wrap img {display:block;height: 100%;font-size:0}
.review_img_wrap .swiper-slide {width:100%}

.layer_popup .oneline_list .wraps.review_list span {display: inline-block;}
.layer_popup .oneline_list .wraps.review_list .review_cont {padding:0;}

.review_img_wrap a {display:block;position:relative}
.review_img_wrap .swiper-pagination {position:absolute;left:50%;margin-left: -30px;bottom:15px;width:60px;padding:2px 0 2px 0;font-size:11px;color:#fff;box-sizing:border-box;z-index:2}
.review_img_wrap .swiper-pagination span{position:relative;z-index:2;}
.review_img_wrap .swiper-container:after{position:absolute;left:50%;margin-left: -30px;bottom:15px;width:60px;height:20px;padding:2px 0 2px 8px;border-radius:11px;background:#030000;box-sizing:border-box;opacity:0.46;z-index:1;content:''}
.review_img_wrap .swiper-pagination-current{font-weight:bold}
.review_img_wrap .more{position:absolute;bottom:15px;right:36px;width:20px;height:20px;padding-top:6px;border-radius:50%;background:#e20265;box-sizing:border-box;text-align:center;z-index:2}
.review_img_wrap .control{position:absolute;right:12px;bottom:15px;width:20px;height:20px;text-align:center}
.review_img_wrap .control .ico{position:relative;margin:6px 0 0 1px;z-index:2}
.review_img_wrap .control.on .ico{width:6px;height:8px;background-position:-83px 0}
.review_img_wrap .control .bg{position:absolute;top:0;left:0;width:20px;height:20px;background:#000;border-radius:50%;opacity:0.71;z-index:1}
		
