.fh_wrap .magazine_item .tag em + em{margin-left:4px}
.fh_wrap .magazine_item .text{margin-top:8px;font-size:14px;color:#5c5c5c;line-height:20px; font-weight: 300; padding:0 9px;}

.box__conner-search { position: relative; padding: 0 10px; border-bottom: 1px solid #f2f2f2; background: #edeef3;}
.for-a11y { overflow: hidden; position: absolute; width: 1px; height: 1px; margin: -1px; padding: 0; border: 0; clip: rect(0 0 0 0);}
.box__conner-search .input__keyword { width: 100%; height: 34px; padding: 0 68px 0 13px; border: 1px solid #e5e5e5; border-radius: 1px; background: #fff;}
.box__conner-search .button__keyword-delete { display: none; position: absolute; top: 0; right: 55px; width: 34px; height: 34px;}
.box__conner-search .del_btn { width: 14px; height: 14px; position: absolute; right: 65px; top: 10px; background-color: transparent; background-image: url(../images/order/order_sp.png); background-repeat: no-repeat; background-size: 150px 230px; background-position: -55px -75px;}
.sp_es { display: inline-block; overflow: hidden; text-indent: -999em;}
.sp_es, .sp_es_before:before, .sp_es_after:after { background-image: url(../images/common/sp_main_new.png); background-size: 500px 500px; background-repeat: no-repeat;}
.box__conner-search .button__submit { position: absolute; top: 0; right: 13px; width: 42px; height: 34px; background-color: transparent;}
.box__conner-search .button__submit .sp_es { position: absolute; top: 7px; right: 11px; width: 22px; height: 22px; background-position: 0px -111px;}
.fh_banner_wrap { width: 100%; background-color: #000; text-align: center; color: #fff; position: relative;}
.fh_banner_wrap img {width: 100%;}
.fh_banner_title { width: 100%; height: 100%; overflow: hidden; z-index: 1; position: absolute;  text-align: center; color: #fff; background: rgba(0,0,0,0.45);}
.fh_banner_title .fh_tt1 { display: block; position: relative; margin-top: 16px; text-shadow: 0 2px 2px rgba(0,0,0,0.45);
    filter: progid:DXImageTransform.Microsoft.Shadow(color=#000, direction=45, strength=2); font-size: 25px;line-height: 30px;
    font-weight: normal; letter-spacing: -0.6px; padding-top: 20px; font-family: NanumMyeongjo, Batang, Myungjo, Serif;}
.fh_banner_title .fh_tt2 { font-size: 14px; font-weight: normal; line-height: 20px; margin-top: 10px; display: block;}

.category .tab_btn{overflow:hidden;width:100%;margin-bottom:10px;padding:9px 0;background:#f4f5f8;white-space:nowrap}
.category .tab_btn .swiper-container{padding:0 12px}
.category .tab_btn button{float:left;height:25px;padding:0 10px;border-radius:12px;background:#f4f5f8;font-size:14px;color:#000;text-align:center;box-sizing:border-box;letter-spacing:-1px; font-weight: 300;}
.category .tab_btn button.on{border-color:#5969a2;background:#5969a2;color:#fff}
.category .tab_btn button + button{margin-left:2px}
.frequently_wrap .review_wrap, .frequently_wrap .new_list_type ul li:first-child {border:none;}
.frequently_wrap .noresult {height: 105px; line-height: 105px; text-align: center; background-color: #fff; font-size: 14px;}
.frequently_best {margin-top:9px;}
