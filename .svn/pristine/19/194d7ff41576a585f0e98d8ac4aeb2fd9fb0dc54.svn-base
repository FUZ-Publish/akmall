/* 공통 - mail */
.mail_wrap{display:-webkit-box;display:-webkit-flex;display:-moz-flex;display:flex;flex-wrap:wrap;-webkit-flex-wrap:wrap;position:relative;padding-bottom: 45px;}
.mail_wrap p{position: relative;display:block;-webkit-box-flex:0 40%;-webkit-flex:0 40%;-moz-flex:0 40%;flex:0 40%;}
.mail_wrap p:first-child:after {content: "@";display: block;position: absolute;right:-7px;top:8px;font-size: 15px;color: #666;} 
.mail_wrap p input{width: 100%;}
.mail_wrap p span{display: block;padding-right: 15px;}
.mail_wrap p:nth-child(2) {-webkit-box-flex:0 60%;-webkit-flex:0 60%;-moz-flex:0 60%;flex:0 60%;}
.mail_wrap p:nth-child(2) span{padding-left: 15px;padding-right: 0;}
.mail_wrap p:nth-child(3){width: 100%;position:absolute;left:0;bottom:0; padding-top: 10px;display:block;-webkit-box-flex:0 100%;-webkit-flex:0 100%;-moz-flex:0 100%;flex:0 100%;} 

/* footer */
.footer_common{padding:9px 0 80px;background: #edeef3;}
.footer_common .footer_menu{background: #6c6c6c;}
.footer_common .footer_menu:after{content:"";display:block;clear:both;}
.footer_common .footer_menu li{float:left;width:33.3%;}
.footer_common .footer_menu li a{display:block;padding:8px 0 7px;border-left:1px solid #767676;border-right:1px solid #5d5d5d;font-size:12px;color:#fff;line-height:20px;text-align:center;}
.footer_common .footer_menu li:first-child{width:33.4%;}
.footer_common .footer_menu li:first-child a{border-left:none;}
.footer_common .footer_menu li:last-child a{border-right:none;}
.footer_common .footer_appdown{display:-webkit-box;display:-moz-box;display:-ms-flexbox;display:-webkit-flex;display:flex;margin:10px 15px 0;}
.footer_common .footer_appdown a{display:block;-webkit-box-flex:1;-webkit-flex:1;-moz-flex:1;flex:1;position:relative;padding:5px 23px 5px 0;margin-left:5px;background: #c1c1c1;font-size:10px;color:#fff;line-height:20px;text-align:center;}
.footer_common .footer_appdown a:first-child{margin-left:0;}
.footer_common .footer_appdown a:after{content:"";position:absolute;right:0;top:0;width:23px;height:30px;border-left:1px solid #b3b3b3;background:#cdcdcd url(../images/common/footer_sp.png) no-repeat -97px -23px;background-size:150px 175px;}
.footer_common address{margin:15px 0 0;font-size:10px;color:#999;line-height:16px;text-align:center;letter-spacing: -1px}
.footer_common address i {margin-left:5px;}
.footer_common address i:before {content:"|";display:inline-block;color:#d5d5d5;padding-right:5px;}
.footer_common .footer_menu2 {margin-top:17px;text-align: center;}
.footer_common .footer_menu2 li {display: inline-block; margin-right:18px;position:relative;}
.footer_common .footer_menu2 li a {color:#666;letter-spacing: -1px}
.footer_common .footer_menu2 li:after {content:"";display:block;width:1px;height:12px;background-color:#c0c0c0;position:absolute;right:-9px;top:3px;}
.footer_common .footer_menu2 li strong {font-weight: bold;}
.footer_common .footer_menu2 li:last-child {margin-right: 0;}
.footer_common .footer_menu2 li:last-child:after {display:none;}
.footer_common .footer_menu2.mt0{margin-top: 0;}
.footer_common .copyright {display:block;margin-top:16px;font-size:10px;color:#999;text-align:center;}

/* 하단 탑 버튼 및 뒤로가기 최근본상품 */
.foot_btn_ara {position:fixed;left:0;bottom:0;z-index:990;width:100%;margin:0;padding:0;width:100%;-webkit-transition:all .3s ease-out 0s;transition:all .3s ease-out 0s;-webkit-transform:translate3d(0,60px,0);transform:translate3d(0,60px,0)}
