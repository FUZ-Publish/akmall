<body>
<!-- wrap -->
<div class="wrap">
	<!-- header -->
	<header>
	</header>
	<!--// header -->
	<!-- container -->
	<div class="container">
	</div>
	<!-- layer_popup -->
	<div class="popWrap">
		<div class="popup">
			<div class="popContain">
				<div class="popCont full"><!-- class :: full -->
					<div class="popHead">
						<h1 class="titH3">비밀번호 찾기</h1>
					</div>
					<div class="popBody">
						<div class="popInner">
							<!-- 본문 (s) -->
							<div class="titArea">
								<h2 class="titH2 normal pointC2">비밀번호 인증 방법을<br> 선택해주세요.</h2>
							</div>
							<div class="formArea">
								<ul class="iptGroup div_1 mt40">
									<li class="mt0">
										<input type="checkbox" name="chk1" id="chk1_1" checked="checked">
										<label for="chk1_1">SMS로 받기   <span>010-1***-5***</span></label>
									</li>
									<li>
										<input type="checkbox" name="chk1" id="chk1_2">
										<label for="chk1_2">이메일로 받기  <span>akbeuty2***@naver.com</span></label>
									</li>
								</ul>

								<span class="label mt40">인증번호</span>
								<div class="iptWrap setBtnS">
									<input type="text" class="ipt" placeholder="인증번호 입력를 입력해주세요" title="인증번호 입력" id="" name="" value="">
									<button type="button" class="btnS">확인</button>
								</div>
								<div class="formMsg">
									인증번호가 발송되었습니다.
									<span class="timeMsg ar">남은 시간 <em>28:30</em></span>
								</div>
							</div>
							<!-- 본문 (e) -->
						</div>
					</div>
					<button type="button" class="btnIco_close"><span class="blind">팝업 닫기</span></button>
				</div>
			</div>
		</div>
	</div>

	<!-- //layer_popup -->
	<script type="text/javascript" src="../resources/js/config/config.js"></script>
	<script type="text/javascript" data-main="config/define_renewalUI.js" src="../resources/js/lib/require.js"></script>
