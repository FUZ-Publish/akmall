	<div class="check">
		<button class="sub_off">서브메뉴 접기</button>
		<input type="checkbox" class="allcheck">
		<a href="#" class="top">TOP ▲</a>
		<!--<button class="check_onoff">하단 메뉴명</button>-->
	</div>
	
</div>
<script>
var $wrap=$('.check div');
	var $box=$('dl');
	var $len = $box.length;
	var $txt = $box.find('dt');

	function item(){
		var htm = '';
	        
	        htm+= ''
	                
	            for(var i= 0, len=$len; i<len; i++){            
	                htm+='<p><input type="checkbox" class="item" checked="checked"> <a href="#idx'+ i + '">' + $txt.eq(i).text() +'</a></p>'
	            };

	        return htm;
	}
	$wrap.append(item());

	for(var i= 0, len=$len; i<len; i++){            
        $box.eq(i).attr('id','idx'+i);
    };


	$(document).on('change','input[type="checkbox"]',function(){

            var $this = jQuery(this);
            var $index = $this.parent().index();

            if($this.is(':checked')){
                $box.eq($index).show();
            }
            if(!$this.is(':checked')){
                $box.eq($index).hide();
     		}

	});

	$('.check_onoff').click(function(){
		var $this = jQuery(this);
        if($this.hasClass('active')){
            $this.removeClass('active');
            $this.next().hide();
        }else if(!$this.hasClass('active')){
            $this.addClass('active');
            $this.next().show();
        }
	});

	$('.sub_off').click(function(){
		var $this = jQuery(this);
        if($this.hasClass('active')){
            $this.removeClass('active');
            $('dd').show();
            $('dt').addClass('active');
        }else if(!$this.hasClass('active')){
            $this.addClass('active');
            $('dd').hide();
            $('dt').removeClass('active');
        }
	});

var dd = $('dd');
var len = dd.length;

$txt.click(function(){
	var $this = jQuery(this);
        if($this.hasClass('active')){
            $this.removeClass('active');
            $this.parent().find('dd').hide();
        }else if(!$this.hasClass('active')){
            $this.addClass('active');
            $this.parent().find('dd').show();
        }
});


$('.num').text(len);

function checkAll(obj1,obj2){
        var state = 1;
        $('.'+obj2).each(function(i,el){
            if($(el).prop('checked')) {state *= 1;}
            else {state *= 0;}
        });
        $('.'+obj1).prop('checked', state);
    }

$(document).on('click', '.item', function(){
	//alert();
	setTimeout( function(){ checkAll('allcheck','item') }, 50); 
}); 
$(document).on('click','.allcheck', function(){
     $('.item').prop('checked', $(this).prop('checked')? true: false);
     if($(this).is(':checked')){
		$box.show();
     }
     if(!$(this).is(':checked')){
     	$box.hide();
     }

});
checkAll('allcheck','item');  
</script>
</body>
