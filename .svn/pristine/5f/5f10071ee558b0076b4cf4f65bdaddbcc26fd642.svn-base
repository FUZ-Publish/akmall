@charset "utf-8";

/*================================================ 01_뷰티APP MAIN ================================================*/

.titleArea .more{position:relative; display:block;}
.titleArea .more:after{content:''; position:absolute; right:0; bottom:10px; width:7px; height:12px; background:url('../images/common/ico_pagelink_arrow.png') no-repeat center; background-size:7px 12px;}
.titleArea .highlight{display:inline-block; position:relative;}
.titleArea .highlight:before{content:''; display:block; position:absolute; bottom:4px; width:100%; height:8px; background-color:#ffa6c4; opacity:0.4; z-index:-1;}

.quizBox{padding:40px 20px 30px; background:#afbdec; border-radius:20px;}
.quizBox .quiz{color:#1b2026; font-size:18px; font-weight:bold; text-align:center; line-height:25px;}
.quizBox .iptWrap{margin-top:18px; text-align:center;}
.quizBox .iptWrap input{width:45px; height:45px; color:#080b10; font-size:24px; line-height:45px; font-weight:bold; text-align:center; text-indent:0; background:#fff; border:2px solid #080b10; border-radius:17px;}
.quizBox .btnArea{margin-top:17px;}

.mainBanner{padding:30px 20px; color:#fff; font-size:18px; line-height:22px; background:url('../images/icon/ico_main_banner.png') no-repeat center right 25px #ffc466; background-size:77px auto;}
.mainBanner .tit{color:#fff;}

.btnArea.mainBtn {text-align:center; margin:40px 0 0 0;}
.btnArea.mainBtn button {width:auto; padding:0 50px;}

.chartList{margin-top:35px;}
.chartList .prdImg{width:100%;}
.chartList .prdImg .img{text-align:center;}
.chartList .prdImg .img img {width:220px; border-radius:0;}
.chartList .prdImg .btnPic{width:40px; height:40px; bottom:-20px;}
.chartList .prdImg .btnPic.off{background-size:40px; }
.chartList .prdImg .btnPic.on{background:url('../images/icon/ico_zzim_on2.png') no-repeat center; background-size:40px; }
.chartList .prdInfo:after{content:''; display:block; clear:both;}
.chartList .txt{display:block; margin-top:12px;}
.chartList .revUser{float:left;}
.chartList .revUser .img img{width:21px; height:21px;}
.chartList .revUser .userInfo{line-height:21px; color:#929699; font-size:13px;}
.chartList .revUser .userInfo em:before{background-color:#929699;}
.chartList .reviewPrd_Luv{float:right; margin-top:10px; font-size:0;}
.chartList .reviewPrd_Luv .luv{display:inline-block; color:#929699; font-size:13px; font-weight:300;}
.chartList .reviewPrd_Luv .luvTime{margin-left:10px; color:#929699; font-size:13px; font-weight:300;}
@media (max-width:320px){
	.chartList .prdImg .img img {width:180px;}
}

.timeSpecial{position:absolute; top:-13px; left:50%; z-index:1; margin-left:-80px; color:#080b10; font-size:14px;}
.timeSpecial em{font-size:36px; font-family: "Montserrat Bold";}
/* 
.akExpList {padding:0 20px;}
.akExpList .swiper-slide:last-child {margin-right:20px;} */
.akExpList .eventList {width:275px; margin-top:0;}
.akExpList .eventList .eventInfo .tit{margin-top:5px;}
.akExpList .eventList .img {overflow:hidden;}
.akExpList .eventList .img,
.akExpList .eventList .img img {position:relative; vertical-align:top; height:190px;}
.akExpList .eventList .img:before {content:''; display:block; position:absolute; top:0; left:0; right:0; bottom:0; width:100%; height:100%; background:rgba(0,0,0,0.04); z-index:1; border-radius:10px;}

.lvuHateArea{padding:40px 20px; background:#ffacbe; }
.lvuHateArea .titleArea{padding:0; font-size:14px;}
.lvuHateArea .titleArea .titH2{margin:0 0 5px; color:#fff; font-family:"Montserrat Bold";}
.lvuHateArea .titleArea .desc{color:#fff;}
.lvuHateArea .prdImg{position:relative; width:220px; height:220px; margin:45px auto 0; background-color:#fff; border-radius:50%;}
.lvuHateArea .prdImg .img img {/* position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); -ms-transform:translate(-50%,-50%); */ border-radius:50%;}
.lvuHateArea .prdImg .tit{position:absolute; top:-20px; right:-20px; display:inline-block; padding:13px; color:#fff; font-size:14px; line-height:18px; background:#ff206c; border:2px solid #000; border-radius:25px 28px 28px 10px;}
.luvHateArea .luvHate{border:1px solid red;}
.luvHate{margin:30px -10px 0;}
.luvHate [class^=reviewPrd_]{font-size:18px; text-align:center;}
.luvHate:after{content:''; display:block; clear:both;}
.luvArea{float:left; width:50%; padding:0 10px;}
.luvArea .reviewPrd_Luv:before{top:-2px; left:50%; margin-left:-33px; display:inline-block; width:22px; height:22px; background-size:22px 22px;}
.hateArea{float:right; width:50%; padding:0 10px;}
.hateArea .reviewPrd_Hate:before{top:-2px; left:50%; margin-left:-33px; display:inline-block; width:22px; height:22px; background-size:22px 22px;}
.luvHate [class$=List]{margin:15px 0 0;}
.luvHate [class$=List] li{padding:25px 30px 21px 20px;}
.luvHate [class$=List] li + li{margin:10px 0 0;}
.luvHate [class$=List] .desc{margin-top:3px; font-size:14px;}
.luvHate [class$=List] .revUser .img{width:16px; height:16px;}
.luvHate [class$=List] .revUser img{width:16px; height:16px; margin-top:-1px;}
.luvHate [class$=List] .userInfo{margin-left:8px; font-size:12px;}
.luvList li{color:#080b10; background:#fff; border-radius:25px 28px 28px 5px;}
.luvList .heartWrap{display: inline-block;}
.luvList .heartRating{display: inline-block; width: 80px; height: 11px; background: url(../images/icon//ico_heart_luv.png) no-repeat left top -1px; background-size: 80px 30px;overflow: hidden;}
.luvList .heartRating > span{display: block; width: auto; height: 11px; background: url(../images/icon//ico_heart_luv.png) no-repeat left bottom; background-size: 77px 30px;}
.luvList .revUser .userInfo em{color:#929699;}
.luvList .revUser .userInfo em:before{background-color:#929699;}
.hateList li{background:#ff8dac; border-radius:25px 28px 5px 28px;}
.hateList li .desc{color:#fff; color:#fff; }
.hateList .revUser .userInfo em{color:#fff;}
.hateList .revUser .userInfo em:before{background-color:#fff;}
.hateList .heartWrap{display: inline-block;}
.hateList .heartRating{display: inline-block; width: 80px; height: 11px; background: url(../images/icon//ico_heart_hate.png) no-repeat left top -1px; background-size: 80px 30px;overflow: hidden;}
.hateList .heartRating > span{display: block; width: auto; height: 11px; background: url(../images/icon//ico_heart_hate.png) no-repeat left bottom; background-size: 77px 30px;}

/* 뷰티앱 메인페이지에 해당 */
.mainWrap .header .gnbHeader .schWrap {left:20px; width:calc(100% - 65px);}
.mainWrap .bestSlide .prdCategory_columnA {width:280px;}
.mainWrap .prdEventArea .prdEventInfo {margin-top:-8px; padding:0 10px;}
.mainWrap .prdEventArea .img {position:relative; vertical-align:top;}
.mainWrap .prdEventArea .img:bofore {content:''; display:block; position:absolute; top:0; left:0; right:0; bottom:0; width:100%; height:100%; background:#f5f6f8; z-index:8;}


/*================================================ 02_V-코스미끄 ================================================*/
/* 코스미끄 서브메인 */
.prdCosmicArea {position:relative; height:200px;}
.prdCosmicArea .img {position:relative;}
.prdCosmicArea .img:after{content:''; position:absolute; top:0; left:0; right:0; bottom:0; width:100%; height:100%; margin:0; background-color:rgba(0,0,0,0.3);}
.prdCosmicArea .img img {height:200px; object-fit:cover;}
.prdCosmicInfo {position:absolute; top:35px; left:20px;}
.prdCosmicInfo .tit {font-size:30px; color:#fff; font-weight:bold; line-height:36px;}
.prdCosmicInfo .txt {font-size:16px; color:#fff; font-weight:300; margin-top:10px;}

section.bgBlack .titleArea [class^=titH],
section.bgBlack .shopLiveList li .inner > .tit,
section.bgBlack .vodScheList .info .tit,
section.bgBlack .prdCategoryVOD .prdTitle,
section.bgBlack .prdCategoryVOD .prdLikeit .btnPic{color:#fff;}
section.bgBlack .vodScheList .info .date {color:#929699;}
section.bgBlack .prdCategoryVOD .prdLikeit .btnPic.off {background:url(../images/icon/ico_zzim_typec2_off.png) no-repeat left 5px; background-size:12px 10px;}

section.bgBlack .shopLiveList + .btnArea {margin:40px 0 0 0;}
section.bgBlack .shopLiveList + .btnArea .btn_l.mini {padding:0 50px;}

/* 방송알림 */
.iptGroup .alarmChk{margin:10px 0 5px; font-size:16px;}

/* 쇼핑라이브 목록 */
.liveAlarm {float:right;}
.liveAlarm [class^=txt] {line-height:28px; margin-right:5px; vertical-align:top;}

.vodScheList {position:relative; margin:0 20px;}
.vodScheList + .vodScheList {margin-top:20px;}
.vodScheList:after{content:''; display:block; clear:both;}
.vodScheList .vodImg{float:left;}
.vodScheList .vodImg .img{width:105px; height:auto;}
.vodScheList .vodImg .img:after{content:none;}
.vodScheList .info{float:left; width:calc(100% - 105px); margin-top:15px; padding-left:15px;}
.vodScheList .info .time{padding-left:22px; font-size:19px; background-size:16px auto;}
.vodScheList .info .tit{margin-top:10px; color:#080b10; font-weight:bold; line-height:22px;}
.vodScheList .btnPic.alarm.off{top:7px; right:0; background:url('../images/icon/ico_alarm_btn2.png') no-repeat center; background-size:30px auto;}
.vodScheList .btnPic.alarm.on{top:7px; right:0;}

.shopLiveList {margin:0 -10px;}
.shopLiveList:after{content:''; display:block; clear:both;}
.shopLiveList li{float:left; width:50%; box-sizing:border-box; padding:0 10px; margin:20px 0 0;}
.shopLiveList li:nth-child(1),
.shopLiveList li:nth-child(2) {margin-top:0;}
.shopLiveList li .inner > .tit{margin-top:12px; color:#080b10; font-weight:bold; line-height:22px; letter-spacing:-0.3px;}
.shopLiveList img{border-radius:20px;}
.shopLiveList .prdInfo{position:absolute; bottom:10px; left:15px; right:15px;}
.shopLiveList .prdInfo:after{content:''; display:block; clear:both;}
.shopLiveList .prdInfo .img{float:left; margin-right:7px;  background:#fff;}
.shopLiveList .prdInfo .img{width:30px; height:30px; object-fit:cover; border-radius:10px;}
.shopLiveList .prdInfo .img:after{content:none;}
.shopLiveList .prdInfo .info{float:left; width:calc(100% - 37px); margin-top:1px; color:#fff; font-size:12px;}
.shopLiveList .prdInfo .info .tit{color:#fff; line-height:16px; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.shopLiveList .prdInfo .info .price{color:#fff; margin-top:-5px;}

.liveTime{position:absolute; top:8px; right:15px; color:#fff;  font-size:13px; font-family:"Montserrat"; z-index:1;}
.btn_l.m.buy{padding:0 15px;}

/* V-VOD상세 */
.vodLiveArea .vodImg .img:after{border-radius:0;}
.vodLiveArea .liveVodInfo {margin:20px 0 0;}
.vodLiveArea .liveVodInfo .vodTitle{display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical; white-space:normal;}
.vodImg .liveBox{position:absolute; top:13px; left:20px;}
.vodImg .liveBox .live{padding:4px 12px; color:#fff; font-size:12px; font-family:"Montserrat Bold"; background-color:#7039eb; border-radius:50px;}
.vodLiveArea .liveVodInfo .hashTag{margin-top:10px; color:#ff206c;}
.vodLiveArea .liveVodInfo .hashTag em{margin-left:5px;}
.vodLiveArea .liveVodInfo .hashTag em:first-child{margin-left:0;}
.liveVodInfo .info{margin-top:10px;}
.liveVodInfo .info .view{margin-left:3px; padding-left:22px; font-size:13px; font-family:"Montserrat"; line-height:20px; background:url('../images/icon/ico_view.png') no-repeat left top 7px; background-size:12px auto;}
.liveVodInfo .info .heart{position:relative;}
.liveVodInfo .info .heart .btnPic {display:inline-block; position:static; width:auto; height:17px; font-size:13px; font-family:"Montserrat"; line-height:17px; padding-left:20px; margin-left:10px; background-position:left top 5px;}

/* 편성표 알림 */
.liveList + .liveList {margin-top:40px;}
.liveList li{margin-top:40px;}
.liveList li:first-child{margin-top:0;}
.time{display:inline-block; padding:2px 0 0 28px; color:#ff206c; font-size:22px; font-weight:bold; background:url('../images/icon/ico_alarm2.png') no-repeat left center; background-size:20px auto;}
.time + .vodImg{margin-top:20px;}
.vodImg{position:relative;}
.vodImg .img{position:relative;}
.vodImg .img:after{content:''; position:absolute; top:0; left:0; width:100%; height:100%; margin:0; background-color:rgba(0,0,0,0.2); border-radius:20px;}
.vodImg .img img{border-radius:20px;}
.vodImg .onAirBox{position:absolute; top:13px; left:10px;}
.vodImg .onAirBox .onAir{padding:4px 12px; color:#fff; font-size:12px; font-family:"Montserrat Bold"; background-color:#ff206c; border-radius:50px;}
.vodImg .onAirBox .onAirTime{margin-left:6px; color:#fff; font-size:13px; font-family:"Montserrat";}
.vodImg.round0 .img img {border-radius:0;}
.vodImg.scrollFixed,
.vodImg.scrollFixed .img img {height:220px;}

.btnPic.alarm{top:15px; right:10px;}
.btnPic.alarm.off{background:url('../images/icon/ico_alarm_btn.png') no-repeat center center; background-size:30px auto;}
.btnPic.alarm.on{background:url('../images/icon/ico_alarm_btnon.png') no-repeat center center; background-size:30px auto;}

.liveVodInfo .vodTitle{margin-top:10px; color:#080b10; font-size:18px; font-weight:bold; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.liveVodInfo .prdPrice{margin-top:15px;}
.liveVodInfo .prdPrice:after{content:''; display:block; clear:both;}
.liveVodInfo .delPrice{display:block; color:#929699; font-size:13px; text-decoration:line-through;}
.liveVodInfo .price{color:#080b10; font-size:22px; font-weight:bold;}
.liveVodInfo .sale{color:#ff206c; font-size:22px; margin-left:5px;}
.liveVodInfo .earlyBuy{float:right; height:35px; margin-top:-10px; padding:0 15px; font-size:15px; line-height:35px;}

/* 편성일 swiper */
.organDayWrap {position:relative; padding:12px 0; background-color:#f5f6f8;}
.organDayWrap:after {content:''; display:block; position:absolute; top:0; right:0; bottom:0; width:35px; background:url(../images/common/bg_tab_bar3.png) no-repeat right bottom; background-size:35px auto; z-index:2;}
.organDayWrap ul {display:flex;}
.organDayWrap li {margin-right:10px;}
.organDayWrap li:first-child {margin-left:5px;}
.organDayWrap li a {display:block; width:30px; text-align:center; padding:10px 12px;}
.organDayWrap li .week {font-size:14px; color:#474c53; font-weight:300;}
.organDayWrap li .day {display:block; font-size:18px; color:#080b10; font-weight:bold;}
.organDayWrap li a.on {background-color:#1b2026; border-radius:50%;}
.organDayWrap li a.on .week {color:#fff; font-weight:bold;}
.organDayWrap li a.on .day {color:#fff;}

/* 영상노출영역 */
.vodArea {margin-top:20px;}

/*================================================ 03_리뷰라운지 ================================================*/
/* 리뷰어 홈 */
.reviewPrdWrap {}

.userRankWrap {padding:20px 0; background-color:#f5f6f8; border-radius:18px;}
.userRankWrap ul {display:table; width:100%;}
.userRankWrap li {display:table-cell; width:33.3%; text-align:center; border-left:1px solid #fff;}
.userRankWrap li:first-child {border-left:0;}
.userRankWrap li .item {display:block; font-size:14px; color:#474c53; margin-bottom:10px;}
.userRankWrap li .val {font-size:18px; color:#080b10; font-weight:bold;}
.userRankWrap + .iptGroup {margin-top:12px;}
.userRankWrap > [class^=titH] {text-align:center; margin-bottom:15px;}
.userRankWrap + [class^=txtList_] {margin-top:30px;}

.reviewPrdOpBox {margin-top:20px; padding:15px 20px; background-color:#f5f6f8; border-radius:14px;}
.reviewPrdOpBox dl:after {content:''; display:block; clear:both;}
.reviewPrdOpBox dt {position:relative; float:left; width:70px; font-size:14px; color:#929699; padding:5px 0;}
.reviewPrdOpBox dd {font-size:14px; color:#1b2026; padding:5px 0;}

[class^=reviewPrd_] {position:relative; font-size:14px; color:#1b2026; line-height:19px; margin-top:15px;}
[class^=reviewPrd_]:before {content:''; display:block; position:absolute; top:1px; left:0; width:16px; height:16px;}
[class^=reviewPrd_] em {display:block; font-size:15px; color:#080b10; font-weight:bold; padding-left:21px;}
[class^=reviewPrd_] em.tit {font-size:18px; color:#1b2026;}
[class^=reviewPrd_] p {padding-left:21px;}
[class^=reviewPrd_] em + p {padding-left:0; margin-top:10px;}
.reviewPrd_Luv:before {background:url(../images/icon/ico_luv.png) no-repeat center center; background-size:16px 16px;}
.reviewPrd_Hate:before {background:url(../images/icon/ico_hate.png) no-repeat center center; background-size:16px 16px;}

.review {margin:20px 0 10px; padding-left:24px; color:#474c53; font-size:13px; background:url(../images/icon/ico_reply.png) no-repeat left top 1px; background-size:16px auto;}

.reviewPrdAid {margin-top:30px;}
.reviewPrdAid:after {content:''; display:block; clear:both;}
.reviewPrdAid .btn_aid {position:relative; width:auto; height:30px; font-size:13px; color:#474c53; font-weight:300; line-height:30px; padding:0 15px 0 33px; border:1px solid #929699; border-radius:16px; margin-right:5px;}
.reviewPrdAid .btn_aid:before {content:''; position:absolute; left:10px; top:50%; width:16px; height:16px; margin-top:-8px; background:url(../images/icon/ico_prd_aid.png) no-repeat center center; background-size:16px 16px;}
.reviewPrdAid .btn_aid.on {color:#fff; background-color:#080b10; border-color:#080b10;}
.reviewPrdAid .btn_aid.on:before {background:url(../images/icon/ico_prd_aid2.png) no-repeat center center; background-size:16px 16px;}

/* 베스트리뷰어는? */
.reviewQnA{margin-top:15px;}
.reviewQnA .faqHeader{padding-left:36px; color:#080b10; font-size:18px; background:url('../images/icon/ico_q.png') no-repeat left center; background-size:18px auto;}
.reviewQnA .faqBody{margin-top:30px; padding-left:36px; color:#474c53; font-size:15px; background:url('../images/icon/ico_a.png') no-repeat left top; background-size:18px auto;}
.blockList.reviewList {margin:20px 0 0 36px;}
.blockList.reviewList li{border:none;}
.blockList.reviewList .method01{background:url('../images/icon/ico_method01.png') no-repeat right 20px center #ffeef4; background-size:45px auto;}
.blockList.reviewList .method02{background:url('../images/icon/ico_method02.png') no-repeat right 20px center #ffeef4; background-size:45px auto;}
.blockList li{border:1px solid #edeff1; border-radius:14px; margin-top:12px; padding:19px 20px;}
.blockList li:first-child{margin-top:0;}
.blockList li .tit{margin-top:6px; color:#080b10; font-size:18px; font-weight:bold;}
.blockList li .desc{margin-top:7px; color:#474c53; font-size:14px;}
.blockList li .desc + button{margin-top:7px;}

/* 베스트리뷰어 */
.bestReviewer{padding-top:5px;}
.bestReviewer li{margin-top:20px;}
.bestReviewer li:first-child{margin-top:0px;}
.bestReviewer .inner {display:flex;}
.bestReviewer .reviewerRnk{width:40px;}
.bestReviewer .reviewerRnk .num{width:25px; height:25px; margin-top:12px; color:#080b10; font-size:12px; text-align:center; line-height:25px;}
.bestReviewer .reviewerRnk .num.first{color:#fff; background:url('../images/icon/ico_ranking_badge1.png') no-repeat; background-size:25px auto;}
.bestReviewer .reviewerRnk .num.Second{color:#fff; background:url('../images/icon/ico_ranking_badge2.png') no-repeat; background-size:25px auto;}
.bestReviewer .reviewerRnk .num.third{color:#fff; background:url('../images/icon/ico_ranking_badge3.png') no-repeat; background-size:25px auto;}
.bestReviewer .reviewerRnk .up{padding-left:15px; background:url('../images/icon/ico_rank_up.png') no-repeat left 4px top 3px; background-size:7px auto; color:#ff206c; font-size:12px; text-align:center;}
.bestReviewer .reviewerRnk .down{padding-left:15px; background:url('../images/icon/ico_rank_down2.png') no-repeat left 4px top 4px; background-size:7px auto; color:#7436ff; font-size:12px; text-align:center;}
.bestReviewer .reviewerRnk .keep {padding-left:9px;}
.bestReviewer .profileImg{width:62px; overflow:hidden; box-sizing:border-box;}
.bestReviewer .profileImg a {display:block;}
.bestReviewer .profileImg img{ width:62px; height:62px; border-radius:50%;}
.bestReviewer .profileImg .img:before {content:''; display:block; position:absolute; top:0; left:0; right:0; bottom:0; width:100%; height:100%; background:rgba(0,0,0,0.04); z-index:1;}
.bestReviewer .reviewer{width:calc(100% - 122px); margin:8px 0 0 20px;}
.bestReviewer .reviewerInfo .name{display:inline-block; font-size:15px; font-weight:bold; vertical-align:middle;}
.bestReviewer .reviewerInfo .info{display:inline-block; margin-left:5px; color:#7436ff; font-size:12px; vertical-align:middle;}
.bestReviewer .reviewerInfo .info > em {display:inline-block; position:relative; margin-left:2px; padding-left:3px;}
.bestReviewer .reviewerInfo .info > em:before {content:''; position:absolute; top:50%; left:0; width:2px; height:2px; margin-top:-2px; background-color:#7436ff;}
.bestReviewer .reviewerInfo .info > em:first-child {margin-left:0; padding-left:0;}
.bestReviewer .reviewerInfo .info > em:first-child:before {content:none;}
.bestReviewer .reviewInfo > span{display:inline-block; color:#929699; font-size:12px; vertical-align:top;}
.bestReviewer .reviewInfo .num{color:#ff206c;}
@media (max-width:320px){
	.bestReviewer .reviewer {margin:2px 0 0 20px;}
	.bestReviewer .reviewerInfo .info {display:block; margin-left:0;}
}

/* 랭킹 배너 */
.rankBanner {position:relative;}
