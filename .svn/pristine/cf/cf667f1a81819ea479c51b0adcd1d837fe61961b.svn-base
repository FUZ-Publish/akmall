<body>
<!-- wrap -->
<div class="wrap">
	<!-- header -->
	<header>
	</header>
	<!--// header -->
	<!-- container -->
	<div class="container">
	</div>
	<!-- layer_popup -->
	<div class="popWrap">
		<div class="popup">
			<div class="popContain">
				<div class="popCont bottom open hasBtn pd0"><!-- class :: bottom / transform class :: open -->
					<div class="popBody">
						<div class="popInner">
							<!-- 본문 (s) -->
							<div class="formArea pd20">
								<div class="iptWrap flex pt30">
									<select name="" id="" class="minSelect">
										<option value="">옵션</option>
									</select>
								</div>
							</div>
							<div class="iptWrap schWrap">
								<input type="text" name="" value="" class="delete sch" placeholder="검색어를 입력해주세요">
								<button type="button" class="schBtn"><span class="blind">검색</span></button>
							</div>

							<div class="formArea pd20">
								<div class="iptGroup">
									<div class="single">
										<input type="checkbox" name="chk3" id="chk3_1">
										<label for="chk3_1">품절제외</label>
									</div>
								</div>
							</div>

							<div class="optionList pd20">
								<ul>
									<li><button type="button" class="option"><img src="../resources/images/content/img_option_default.png" alt=""><span class="tit">13N 아이보리 <br>(남은수량 : 709개)</span></button></li>
									<li><button type="button" class="option"><img src="../resources/images/content/img_option_default.png" alt=""><span class="tit">17N 바닐라 <br>(남은수량 : 709개)</button></li>
									<li><button type="button" class="option soldOut"><img src="../resources/images/content/img_option_default.png" alt=""><span class="tit">21C 쿨 베이지 <br>[품절]</span></button></li>
									<li><button type="button" class="option"><img src="../resources/images/content/img_option_default.png" alt=""><span class="tit">21N 베이지 <br>(남은수량 : 709개)</span></button></li>
									<li><button type="button" class="option"><img src="../resources/images/content/img_option_default.png" alt=""><span class="tit">23C 쿨 샌드 <br>(남은수량 : 709개)</span></button></li>
								</ul>
							</div>
							<!-- 본문 (e) -->
						</div>
					</div>
					
					<!-- <button type="button" class="btnIco_close"><span class="blind">팝업 닫기</span></button> -->
				</div>
			</div>
		</div>
